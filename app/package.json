{
  "name": "webpack-setup",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "type:check": "tsc --noEmit",
    "type:check:watch": "npm run type-check -- --watch",
    "lint": "npm run lint:sources && npm run lint:styles",
    "lint:sources": "eslint './src/**/*.ts' --max-warnings=0",
    "lint:sources:fix": "npm run lint:sources -- --fix",
    "lint:styles": "stylelint 'src/**/*.css' 'src/**/*.pcss'",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "build:dev": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=development webpack --config ./webpack/webpack.config.dev.ts",
    "start": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.dev.ts",
    "build:prod": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=production webpack --config ./webpack/webpack.config.ts",
    "stats": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=production webpack --config ./webpack/webpack.config.ts --profile --json > ./dist/stats.json",
    "analyze:ci": "npm run build:prod && npm run analyze --prefix ../analysis",
    "analyze": "npm run stats && npm run analyze --prefix ../analysis && npm run display --prefix ../analysis",
    "serve": "npm run serve --prefix ../analysis",
    "serve:https": "npm run serve:https --prefix ../analysis",
    "lighthouse": "npm run lighthouse --prefix ../analysis",
    "favicons": "npm start --prefix ../favicon-generator",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@babel/runtime-corejs3": "7.9.2",
    "@types/copy-webpack-plugin": "5.0.0",
    "@types/html-webpack-plugin": "3.2.2",
    "@types/mini-css-extract-plugin": "0.9.1",
    "@types/node": "13.9.5",
    "@types/terser-webpack-plugin": "2.2.0",
    "@types/webpack": "4.41.8",
    "@types/webpack-dev-server": "3.10.1",
    "@types/webpack-manifest-plugin": "2.1.0",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.4",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.0.2",
    "mini-css-extract-plugin": "0.9.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "regenerator-runtime": "0.13.5",
    "style-loader": "1.1.3",
    "stylelint": "13.2.1",
    "stylelint-config-standard": "20.0.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-node": "8.8.1",
    "typescript": "3.8.3",
    "url-loader": "4.0.0",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0"
  }
}
