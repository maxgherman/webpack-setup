{
  "name": "webpack-setup",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "type:check": "tsc --noEmit",
    "type:check:watch": "npm run type-check -- --watch",
    "lint": "npm run lint:sources && npm run lint:styles",
    "lint:sources": "eslint './src/**/*.ts' --max-warnings=0",
    "lint:sources:fix": "npm run lint:sources -- --fix",
    "lint:styles": "stylelint 'src/**/*.css' 'src/**/*.pcss'",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "build:dev": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=development webpack --config ./webpack/webpack.config.dev.ts",
    "start": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=development webpack serve --config ./webpack/webpack.config.dev.ts",
    "build:prod": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=production webpack --config ./webpack/webpack.config.ts",
    "stats": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=production webpack --config ./webpack/webpack.config.ts --profile --json > ./dist/stats.json",
    "analyze:ci": "npm run build:prod && npm run analyze --prefix ../analysis",
    "analyze": "npm run stats && npm run analyze --prefix ../analysis && npm run display --prefix ../analysis",
    "serve": "npm run serve --prefix ../analysis -- http://localhost:8080 ../app/dist",
    "serve:https": "npm run serve:https --prefix ../analysis -- https://localhost:8080 ../app/dist",
    "lighthouse": "npm run lighthouse --prefix ../analysis",
    "favicons": "npm start --prefix ../favicon-generator",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.15",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@babel/runtime-corejs3": "7.12.13",
    "@types/copy-webpack-plugin": "6.4.0",
    "@types/html-webpack-plugin": "3.2.4",
    "@types/mini-css-extract-plugin": "1.2.2",
    "@types/node": "14.14.25",
    "@types/terser-webpack-plugin": "5.0.2",
    "@types/webpack-dev-server": "3.11.1",
    "@types/webpack-env": "^1.16.0",
    "@types/webpack-manifest-plugin": "3.0.3",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "7.0.0",
    "core-js": "3.8.3",
    "cross-env": "7.0.3",
    "css-loader": "5.0.2",
    "cssnano": "4.1.10",
    "eslint": "7.19.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.0.0",
    "mini-css-extract-plugin": "1.3.6",
    "postcss-loader": "5.0.0",
    "postcss-preset-env": "6.7.0",
    "regenerator-runtime": "0.13.7",
    "style-loader": "2.0.0",
    "stylelint": "13.9.0",
    "stylelint-config-standard": "20.0.0",
    "terser-webpack-plugin": "5.1.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.4",
    "url-loader": "4.1.1",
    "webpack": "5.21.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2",
    "webpack-manifest-plugin": "3.0.0"
  },
  "dependencies": {
    "date-fns": "^2.17.0"
  }
}
