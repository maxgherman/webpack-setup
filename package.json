{
  "name": "webpack-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "eslint './src/**'",
    "lint:fix": "npm run lint -- --fix",
    "build:dev": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=development webpack --config ./webpack/webpack.config.dev.ts",
    "start": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.dev.ts",
    "build:prod": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig.json\" NODE_ENV=production webpack --config ./webpack/webpack.config.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@babel/runtime-corejs3": "7.9.2",
    "@types/clean-webpack-plugin": "0.1.3",
    "@types/html-webpack-plugin": "3.2.2",
    "@types/node": "13.9.3",
    "@types/webpack": "4.41.8",
    "@types/webpack-dev-server": "3.10.1",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.6.4",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "4.0.1",
    "regenerator-runtime": "0.13.5",
    "ts-node": "8.8.1",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
